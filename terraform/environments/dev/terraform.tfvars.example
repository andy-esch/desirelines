# Development environment configuration
# Copy this file to terraform.tfvars and customize for your GCP project

gcp_project_id     = "your-gcp-project-id"
gcp_project_number = "your-gcp-project-number"  # Found in GCP Console project settings
gcp_region         = "us-central1"              # Choose your preferred region

function_source_tag = "abc1234"                 # Git SHA for function source packages

# Developer access
developer_email = "your-email@example.com"     # For BigQuery console access
