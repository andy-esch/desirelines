# Example environment file for local development
# Copy this to .env and fill in your values

# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_BUCKET_NAME=your-bucket-name
GCP_PUBSUB_TOPIC_PATH=projects/your-gcp-project-id/topics/strava-webhook-events

# Strava API Configuration
STRAVA_CLIENT_ID=your_client_id
STRAVA_CLIENT_SECRET=your_client_secret
STRAVA_REFRESH_TOKEN=your_refresh_token

# Strava Webhook Configuration (for dispatcher)
STRAVA_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token
STRAVA_WEBHOOK_SUBSCRIPTION_ID=123456

# BigQuery Configuration (for BQ inserter)
GCP_BIGQUERY_DATASET=local_testing

# Live Environment Overrides (optional - set these for live testing)
# GCP_PROJECT_ID_LIVE=your-live-project-id
# GCP_BUCKET_NAME_LIVE=your-live-bucket-name
# STRAVA_CLIENT_ID_LIVE=your_live_client_id
# STRAVA_CLIENT_SECRET_LIVE=your_live_client_secret
# STRAVA_REFRESH_TOKEN_LIVE=your_live_refresh_token

# PubSub Emulator (automatically set by docker-compose)
# PUBSUB_EMULATOR_HOST=pubsub-emulator:8085
